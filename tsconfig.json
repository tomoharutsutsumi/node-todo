    "compilerOptions": {
        "target": "es6",
        "module": "es6",
        "moduleResolution": "Node",
        "outDir": "./dist",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true
    },
    "include": ["models/**/*", "Todo.js"],
    "exclude": ["node_modules"]
}