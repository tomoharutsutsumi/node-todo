{
    "compilerOptions": {
        "target": "es6",
        "module": "es2020",
        "moduleResolution": "Node",
        "outDir": "./dist",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true
    },
    "include": ["src"],
    "exclude": ["node_modules"]
}